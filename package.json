{
  "name": "js-schemathesis",
  "version": "1.0.0",
  "description": "Property-based API testing framework for TypeScript/JavaScript - similar to schemathesis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "start": "npm run build && node dist/cli.js",
    "dev": "tsc && node --watch dist/cli.js",
    "example": "npm run build && node dist/examples/basic-usage.js",
    "example:advanced": "npm run build && node dist/examples/advanced-usage.js",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^11.0.0",
    "js-yaml": "^4.1.0",
    "ajv": "^8.12.0",
    "fast-check": "^3.15.0",
    "chalk": "^5.3.0",
    "openapi-types": "^12.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/js-yaml": "^4.0.0",
    "@types/jest": "^29.5.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0"
  },
  "keywords": [
    "api",
    "testing",
    "property-based",
    "openapi",
    "swagger",
    "fuzzing",
    "automation",
    "typescript"
  ],
  "author": "JS-Schemathesis",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
